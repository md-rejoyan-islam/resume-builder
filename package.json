{
  "name": "resume-builder",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "format": "pnpm --filter ./server format",
    "format:fix": "pnpm --filter ./server format:fix",
    "lint": "pnpm --filter ./server lint",
    "lint:fix": "pnpm --filter ./server lint:fix",
    "prepare": "husky && husky install",
    "build": "pnpm --filter ./server build && pnpm --filter ./client build",
    "install": "pnpm --filter ./server install && pnpm --filter ./client install",
    "start": "pnpm --filter ./server start & pnpm --filter ./client start",
    "dev": "pnpm --filter ./server dev & pnpm --filter ./client dev"
  },
